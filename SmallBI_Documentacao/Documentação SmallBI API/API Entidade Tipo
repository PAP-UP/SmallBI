Documentação SmallBI API

Entidade Tipo:
url: "http://backend.smallbi.com.br:18080/SmallBI_WebService/rest/tipo/"

Listar: 
Método: GET
Path: "http://backend.smallbi.com.br:18080/SmallBI_WebService/rest/tipo/listar"
Retorno:
  {
    "descricao": "Cobrança",
    "idTipo": 1
  }

Adicionar:
Método: POST
Path: "http://backend.smallbi.com.br:18080/SmallBI_WebService/rest/tipo/adicionar"
Parametros: "application/json"
Exemplo:
{
	"usuarioId": 1,
	"descricao": "Comercial"
}

Alterar:
Método: POST
Path: "http://backend.smallbi.com.br:18080/SmallBI_WebService/rest/tipo/alterar"
Parametros: "application/json"
Exemplo:
{
	"idTipo":1,
	"usuarioId": 1,
	"descricao": "Comercial"
}

Deletar:
Método: DELETE
Path: "http://backend.smallbi.com.br:18080/SmallBI_WebService/rest/cidade/deletar/{id}"

GetById:
Método: GET
Path: "http://backend.smallbi.com.br:18080/SmallBI_WebService/rest/tipo/getById/{idTipo}"
Parametros: "application/json"
Exemplo: 
http://backend.smallbi.com.br:18080/SmallBI_WebService/rest/tipo/getById/1
Retorno:
  {
    "descricao": "Cobrança",
    "idTipo": 1
  }